{
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 5
  },
  "rules": {
    "strict": 0,
    "brace-style": [2, "1tbs", { "allowSingleLine": true}],
    "camelcase": 2,
    "comma-style": [2, "last"],
    "consistent-this": [2, "self"],
    "new-cap": 0,
    "no-extra-semi": 2,
    "no-lonely-if": 2,
    "no-underscore-dangle": 0,
    "no-spaced-func": 2,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [2, "always"]
  }
}
